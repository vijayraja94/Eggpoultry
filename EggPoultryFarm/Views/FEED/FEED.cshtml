        <script>
    function getinput() {
        //var username = $(".clsTxt").val()
        //var Password = $("#InputPassword1").val()

        var input = {
            Month: $("#Month").val(),
            DATE: $("#DATE").val(),
            Year: $("#Year").val(),
            FEEDQUALITY: $("#FEEDQUALITY").val(),
            FEEDWEIGHT: $("#FEEDWEIGHT").val(),
            FEEDPRICE: $("#FEEDPRICE").val()
        }
        $.ajax({

            type: "post",
            url: '@Url.Action("FEED", "FEED")',
            data: JSON.stringify(input),
            dataType: "json",
            async: false,
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                debugger
                if (data.status == "01") {
                    //alert(data.Message);

                    window.location.href = '@Url.Action("FEED", "FEED")';
                }
                else {
                    alert(data.Message);
                }
            },
            error: function (ex)
            {
            },
        })
    }


        </script>
        
   
